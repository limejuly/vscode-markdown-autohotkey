{
  "name": "vscode-markdown-autohotkey",
  "displayName": "markdown autohotkey",
  "description":  "Markdown fenced code block support for autohotkey",
  "version": "0.0.1",
  "publisher": "limejuly",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/limejuly/vscode-markdown-autohotkey.git"
  },
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "autohotkey-injection"
      }
    ],
    "grammars": [
      {
        "language": "autohotkey-injection",
        "scopeName": "markdown.autohotkey.codeblock",
        "path": "./syntaxes/codeblock.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.autohotkey": "autohotkey"
        }
      }
    ]
  },
  "extensionDependencies":[
     "vscode-autohotkey"
  ]
}
